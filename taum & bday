#include<stdio.h>
void main()
{
    int b,w,a,t1;
    scanf("%d %d",&b,&w);
    int x,y,z;
    scanf("%d %d %d",&x,&y,&z);
    if(x<=z && y<=z)
    {
        a=(b*x)+(w*y);
        printf("%d",a);
    }
    else if(x<=z ||y<=z && x<y)
    {
        t1=z+x;
        a=(b*t1)+(w*z);
        printf("%d",a);
    }
        else
        {
        t1=z+y;
        a=(b*t1)+(w*z);
        printf("%d",a);
        }
    }
